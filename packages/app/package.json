{
  "name": "@wowarenalogs/app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "build:app": "shx rm -rf dist && tsc -p . && webpack --config webpack.config.js && webpack --config webpack.preload.config.js && shx cp -r public dist/public",
    "genPreload": "cross-env ts-node script/generatePreload.ts && npx prettier src/preloadApi.ts --write && npx prettier src/windowApi.d.ts --write",
    "release:app": "git checkout release/wowarenalogs/app && git pull && git merge --no-edit --no-ff main && git push && git checkout main"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-loader": "^8.2.5",
    "eslint-config-wowarenalogs": "*",
    "shx": "^0.3.4",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@wowarenalogs/parser": "^5.2.0",
    "@wowarenalogs/shared": "^1.0.0",
    "chokidar": "^3.5.3",
    "electron": "17.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-updater": "^5.3.0",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.7",
    "node-loader": "^2.0.0",
    "update-electron-app": "^2.0.1",
    "webpack": "^5.72.0"
  }
}
