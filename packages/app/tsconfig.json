{
  "exclude": ["script/generatePreload.ts", "dist"],
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "noEmitOnError": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "esModuleInterop": true
  }
}
